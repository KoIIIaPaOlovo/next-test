{
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "start": "next start",
    "build-start": "pnpm run build && pnpm run start",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "dotenv -e .env.test start-server-and-test start http://localhost:3000 'cypress run --spec \"cypress/e2e/auth/**/*.cy.{js,ts,jsx,tsx}\"'",
    "test:e2e:smoke": "dotenv -e .env.test start-server-and-test start http://localhost:3000 'cypress run --spec \"cypress/e2e/auth/**/smoke/**/*.cy.{js,ts,jsx,tsx}\"'",
    "test:e2e:full": "dotenv -e .env.test -- pnpm run build && dotenv -e .env.test -- start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:open": "dotenv -e .env.test -- pnpm run build && dotenv -e .env.test -- start-server-and-test start http://localhost:3000 cy:open",
    "test:dev": "dotenv -e .env.test start-server-and-test dev http://localhost:3000 cy:run"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "10.4.20",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "next": "latest",
    "next-auth": "5.0.0-beta.30",
    "postcss": "8.5.1",
    "postgres": "^3.4.6",
    "react": "latest",
    "react-dom": "latest",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "use-debounce": "^10.0.4",
    "zod": "^3.25.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "22.10.7",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "cypress": "^15.9.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "start-server-and-test": "^2.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "cypress",
      "unrs-resolver"
    ]
  }
}
